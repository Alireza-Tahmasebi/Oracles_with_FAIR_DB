{
  "data_path": "datasets/adult.csv",

  "MissingValueHandling": {
    "default": {
      "missing_values": ["?", "NA", "N/A", "null", "None", "-", "", "nan", "NaN", "missing"]
    },
    "custom": {
      "missing_values": ["---", "UNKNOWN", "999"]
    }
  },


  "irrelevant_features": ["marital-status", "capital-gain", "capital-loss", "occupation", "relationship", "fnlwgt"],
  

  "discretization": {
  "age": {
    "bins": [15, 30, 45, 60, 75, 90],
    "labels": ["15-30", "30-45", "45-60", "60-75", "75-90"]
  },
  "hours-per-week": {
    "bins": [0, 20, 40, 60, 80, 100],
    "labels": ["0-20", "21-40", "41-60", "61-80", "81-100"]
   }
  }
}