{
    "log_level": "DEBUG",
  
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "standard": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      }
    },

    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "WARNING",
        "formatter": "standard",
        "stream": "ext://sys.stdout"
      },

      "file": {
        "class": "logging.FileHandler",
        "level": "DEBUG",
        "formatter": "standard",
        "filename": "logs/pipeline.log",
        "mode": "w"
      }
    },
    
    "root": {
      "level": "DEBUG",
      "handlers": ["console", "file"]
    }
  }
  